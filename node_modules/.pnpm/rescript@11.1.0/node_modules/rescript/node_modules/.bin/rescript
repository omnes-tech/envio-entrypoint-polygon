#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/gustavodeps/Documents/GitHub/envio-entrypoint-polygon/node_modules/.pnpm/rescript@11.1.0/node_modules/rescript/node_modules:/Users/gustavodeps/Documents/GitHub/envio-entrypoint-polygon/node_modules/.pnpm/rescript@11.1.0/node_modules:/Users/gustavodeps/Documents/GitHub/envio-entrypoint-polygon/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/gustavodeps/Documents/GitHub/envio-entrypoint-polygon/node_modules/.pnpm/rescript@11.1.0/node_modules/rescript/node_modules:/Users/gustavodeps/Documents/GitHub/envio-entrypoint-polygon/node_modules/.pnpm/rescript@11.1.0/node_modules:/Users/gustavodeps/Documents/GitHub/envio-entrypoint-polygon/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../rescript" "$@"
else
  exec node  "$basedir/../../rescript" "$@"
fi
